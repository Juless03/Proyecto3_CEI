let
  var i: Integer;
  var j: Integer;
  var k: Integer;
  var num: Integer
in
  begin
    getint(var num); ! Number of iterations.
    i := 1;
    j := 2;

    repeat
      begin
        putint(i);
        put(' ');
        k := i + j;
        i := j;
        j := k;
        num := num - 1;
      end
    until num <= 0;
  end

